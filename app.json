{
  "expo": {
    "name": "nuFit",
    "slug": "nuFit",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#400e02"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.samirazein20.nuFit",
